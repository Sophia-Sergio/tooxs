<%= simple_form_for(@r) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :store %>
    <%= f.association :department %>
    <%= f.input :date %>
    <%= f.input :year %>
    <%= f.input :month %>
    <%= f.input :week %>
    <%= f.input :dow %>
    <%= f.input :nine %>
    <%= f.input :ten %>
    <%= f.input :eleven %>
    <%= f.input :twelve %>
    <%= f.input :thirteen %>
    <%= f.input :fourteen %>
    <%= f.input :fifteen %>
    <%= f.input :sixteen %>
    <%= f.input :seventeen %>
    <%= f.input :eighteen %>
    <%= f.input :nineteen %>
    <%= f.input :twenty %>
    <%= f.input :twenty_one %>
    <%= f.input :twenty_two %>
    <%= f.input :twenty_three %>
    <%= f.input :total_day %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
