<%= simple_form_for(@staffing_case) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :id_case %>
    <%= f.input :tolerance %>
    <%= f.input :actual_staffing_eval %>
    <%= f.input :max_time %>
    <%= f.input :user %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
