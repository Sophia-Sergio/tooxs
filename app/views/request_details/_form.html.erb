<%= simple_form_for(@request_detail) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :request_id %>
    <%= f.input :department_id %>
    <%= f.input :turn %>
    <%= f.input :quantity %>
    <%= f.input :rrhh %>
    <%= f.input :internal %>
    <%= f.input :laborum %>
    <%= f.input :trabajando %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
